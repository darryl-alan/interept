<h1 class="text-center"><%= "Category: #{@category.name}" %></h1>
<div class='text-center'>
  <% if logged_in? && current_user.admin? %>
    <%= link_to "Edit", edit_category_path(@category), class: "btn btn-primary btn-lg" %>
  <% end %>
  <%= link_to "Back", categories_path, class: 'btn btn-info btn-lg' %>
</div>
<div class='text-center'><%= will_paginate @category_reports %></div>
<%= render 'reports/reports', reports: @category_reports %>
<div class='text-center'><%= will_paginate @category_reports %></div>
